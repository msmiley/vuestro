{
  "name": "vuestro",
  "version": "8.2.1",
  "description": "Vue.js 2 Components",
  "author": "Mike Smiley <msmiley@alumni.rice.edu>",
  "url": "https://github.com/msmiley/vuestro",
  "repository": "msmiley/vuestro.git",
  "license": "ISC",
  "main": "./src/plugin/index.js",
  "scripts": {
    "dev": "webpack-dev-server -d --hot --port 8088 --config ./build/webpack.dev.config.js",
    "build": "webpack --config ./build/webpack.prod.config.js",
    "build-pages": "webpack --config ./build/webpack.pages.config.js"
  },
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.7.0",
    "brace": "^0.11.1",
    "d3": "^5.16.0",
    "d3-sankey": "^0.12.3",
    "geopattern": "^1.2.3",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "vue-awesome": "^4.1.0",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/test-utils": "^1.0.5",
    "axios": "^0.21.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.0",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-awesome)/)"
    ]
  },
  "keywords": [
    "Vue",
    "gui",
    "ux",
    "framework",
    "widgets",
    "ui"
  ]
}
